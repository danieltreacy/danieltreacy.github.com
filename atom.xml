---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Daniel Treacy - Blog</title>
  <link href="http://danieltreacy.com/"/>
  <link type="application/atom+xml" rel="self" href="http://danieltreacy.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://danieltreacy.com/</id>
  <author>
    <name>Daniel Treacy</name>
    <email>daniel.t.treacy@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://danieltreacy.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://danieltreacy.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Daniel Treacy</name>
      <uri>http://danieltreacy.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>